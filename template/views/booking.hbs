    <div class="nav-links" id="navLinks">
        <i class="fa-solid fa-xmark" onclick="hideMenu()"></i>
        <ul>
            <li><a href="/front">Home</a></li>
            <li><a href="/tour">Tour</a></li>
            <li><a href="/booking">Booking</a></li>
            <li><a href="/contact">Contact Us</a></li>
