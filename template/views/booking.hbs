        </ul>
    </div>
    <i class="fa-solid fa-bars" onclick="showmenu()"></i>
    
</nav>
</section>

<table id="customers">
    <tr>
      <th>Place</th>
      <th>Date</th>
      <th>Time booked</th>
      <th>Tickets</th>
    </tr>
      {{# each Bookings}}
      <tr>
        <td>{{ email }}</td>
        <td>{{date}}</td>
        <td>{{times}}</td>
        <td>{{ticket}}</td>
      </tr>
      {{/each}}
  </table>
